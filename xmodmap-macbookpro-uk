! caps
remove Lock = Caps_Lock
add Control = Caps_Lock
! enter (next to right cmd)
keycode 108 = Multi_key
! right cmd
keycode 116 = Down

! left of 1
keycode 49 = section plusminus
! left of z
keycode 94 = grave asciitilde

! Accented characters
keycode 38 = a A aacute Aacute
keycode 26 = e E eacute Eacute
keycode 31 = i I iacute Iacute
keycode 32 = o O oacute Oacute
keycode 30 = u U uacute Uacute
keycode 54 = c C ccedilla Ccedilla
keycode 57 = n N ntilde Ntilde
keycode 39 = s S ssharp
keycode 28 = t T thorn Thorn
keycode 40 = d D eth Eth

! Mac-style punctuation
keycode 34 = bracketleft braceleft leftdoublequotemark rightdoublequotemark
keycode 35 = bracketright braceright leftsinglequotemark rightsinglequotemark
keycode 47 = semicolon colon ellipsis
keycode 20 = minus underscore endash emdash
keycode 61 = slash question questiondown

! Number row
keycode 10 = 1 exclam exclamdown
keycode 11 = 2 at EuroSign
keycode 12 = 3 numbersign sterling
keycode 13 = 4 dollar cent
keycode 17 = 8 asterisk degree

